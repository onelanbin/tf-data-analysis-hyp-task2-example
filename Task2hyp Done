import pandas as pd
import numpy as np


chat_id = 1266169265 # Ваш chat ID, не меняйте название переменной

from scipy.stats import ks_2samp

def test_randomizer(sample1, sample2):
    p_value = ks_2samp(sample1, sample2)[1]
    if p_value < 0.03:
        return True
    else:
        return False
